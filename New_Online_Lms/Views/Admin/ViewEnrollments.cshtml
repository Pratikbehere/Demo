@{
    ViewBag.Title = "Student Enrollments";
}

<h2>Student Enrollments</h2>

<table class="table">
    <thead>
        <tr>
            <th>Student Name</th>
            <th>Course Title</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var enrollment in Model)
        {
            <tr>
                <td>@enrollment.Student.Name</td>
                <td>@enrollment.Course.Title</td>
               
            </tr>
        }
    </tbody>
</table>
